{
    "version"                   : "0.3.0",
    "admins"                    : ["Username#1234"],
    "activation_str"            : "\\",
    "description"               : "A retro TTS bot for Discord (Beta)\nVisit https://github.com/naschorr/hawking",
    "announce_updates"          : true,
    "delete_commands"           : true,
    "channel_timeout"           : 900,
    "skip_votes"                : 3,
    "skip_percentage"           : 33,

    "debug_level"               : 1,
    "token_file"                : "token.json",
    "_token_file_path"          : "",
    "phrases_file_extension"    : "json",
    "phrases_folder"            : "phrases",
    "_phrases_folder_path"      : "",
    "tts_file"                  : "say.exe",
    "_tts_file_path"            : "",
    "tts_output_dir"            : "temp",
    "_tts_output_dir_path"      : "",
    "ffmpeg_before_options"     : "-ac 2",
    "ffmpeg_options"            : "-loglevel 16",
    "output_extension"          : "wav",
    "wine"                      : "wine",
    "xvfb_prepend"              : "DISPLAY=:0.0",
    "headless"                  : false,
    "modules_folder"            : "modules",

    "prepend"                   : "[:phoneme on]",
    "append"                    : "",
    "char_limit"                : 1250,
    "newline_replacement"       : "[_<250,10>]",
    "replace_emoji"             : true,

    "bpm"                       : 100,
    "octave"                    : 2,
    "tone"                      : false,
    "bad"                       : false,
    "bad_percent"               : 10,

    "boto_enable"               : false,
    "boto_resource"             : "dynamodb",
    "boto_region_name"          : "us-east-2",
    "boto_table_name"           : "Hawking",
    "boto_primary_key"          : "QueryId"
}
